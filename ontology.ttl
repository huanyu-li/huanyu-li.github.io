###  https://w3id.org/cocoon/v1.0.1#CDNService
:CDNService rdf:type owl:Class ;
            rdfs:subClassOf :PaaS .


###  https://w3id.org/cocoon/v1.0.1#CloudNetworkPriceSpecification
:CloudNetworkPriceSpecification rdf:type owl:Class ;
                                rdfs:subClassOf :CloudServicePriceSpecification ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :forUsageLessEqual ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass <https://schema.org/TypeAndQuantityNode>
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :forUsageMoreThan ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass <https://schema.org/TypeAndQuantityNode>
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :specialRateType ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onDataRange xsd:string
                                                ] ;
                                rdfs:comment "This class can be used to model network services prices, including internet egress traffic, load balancing forwarding rules."@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CloudNetworkPriceSpecification ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :specialRateType ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:string
                       ] ;
   rdfs:comment "i.e. Egress between zones in the same region; Egress between regions within the US; Egress to Google products (such as YouTube, Maps, Drive)"@en
 ] .


###  https://w3id.org/cocoon/v1.0.1#CloudOSPriceSpecification
:CloudOSPriceSpecification rdf:type owl:Class ;
                           rdfs:subClassOf :CloudServicePriceSpecification ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :chargedPerCore ;
                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onDataRange xsd:boolean
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :forCoresLessEqual ;
                                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onDataRange xsd:decimal
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :forCoresMoreThan ;
                                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onDataRange xsd:decimal
                                           ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CloudOSPriceSpecification ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :forCoresMoreThan ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:decimal
                       ] ;
   rdfs:comment "Describe  the price  for  machines  with  more  than  specified  number  of  cores."@en
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CloudOSPriceSpecification ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :chargedPerCore ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:boolean
                       ] ;
   rdfs:comment """For example on Google Cloud, Windows Server images on some machine types are charged based on the number of CPUs, i.e., n1-standard-4, n1-highcpu-4, and n1-highmem-4 are machine-types with 4 vCPUs,
and are charged at $0.16 USD/hour (4 times $0.04 USD/hour).

https://github.com/miranda-zhang/cloud-computing-schema/blob/master/example/gcloud/os.md#windows-server-images"""@en
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CloudOSPriceSpecification ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :forCoresLessEqual ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:decimal
                       ] ;
   rdfs:comment "Describe price for machines with less than orequal to the specified number of cores."@en
 ] .


###  https://w3id.org/cocoon/v1.0.1#CloudService
:CloudService rdf:type owl:Class ;
              rdfs:subClassOf :Service ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://purl.org/goodrelations/v1#hasPriceSpecification> ;
                                owl:someValuesFrom <http://purl.org/goodrelations/v1#UnitPriceSpecification>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasProvider ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass <http://purl.org/goodrelations/v1#BusinessEntity>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :inRegion ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Region
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :inZone ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:string
                              ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CloudService ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty <http://purl.org/goodrelations/v1#hasPriceSpecification> ;
                         owl:someValuesFrom <http://purl.org/goodrelations/v1#UnitPriceSpecification>
                       ] ;
   rdfs:comment "Price specification for Cloud Service"@en
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CloudService ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :inRegion ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Region
                       ] ;
   rdfs:comment "Region can either be associated with price or a type of service. So Region can be specified either within CloudService class and subclasses, or in UnitPriceSpecification. For example, when the service has different priced in different regions, regions are associated with price; when there are lots of measurements for the service in a region, region can be associated with service."@en
 ] .


###  https://w3id.org/cocoon/v1.0.1#CloudServicePriceSpecification
:CloudServicePriceSpecification rdf:type owl:Class ;
                                rdfs:subClassOf <http://purl.org/goodrelations/v1#UnitPriceSpecification> ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :inRegion ;
                                                  owl:someValuesFrom :Region
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasCurrencyValue ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onDataRange xsd:decimal
                                                ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CloudServicePriceSpecification ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :inRegion ;
                         owl:someValuesFrom :Region
                       ] ;
   rdfs:comment "CloudNetworkPriceSpecification use other properties to sepecify destination"@en
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CloudServicePriceSpecification ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :hasCurrencyValue ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:decimal
                       ] ;
   rdfs:comment "gr:hasCurrencyValue seems only allow float value, extend to allow xsd:decimal, which can represent exact monetary values. Floats can introduce cumulative rounding errors."@en
 ] .


###  https://w3id.org/cocoon/v1.0.1#CloudStorageTransactionsPriceSpecification
:CloudStorageTransactionsPriceSpecification rdf:type owl:Class ;
                                            rdfs:subClassOf :CloudServicePriceSpecification ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.org/goodrelations/v1#hasPriceSpecification> ;
                                                              owl:someValuesFrom <http://purl.org/goodrelations/v1#UnitPriceSpecification>
                                                            ] ;
                                            rdfs:comment """Can be used to define a common price spec for multiple cloud service individuals,
i.e. the storage transaction spec has a different price in a different region,
but there is a common transaction price spec for a group of cloud storage offers."""@en .


###  https://w3id.org/cocoon/v1.0.1#ComputeService
:ComputeService rdf:type owl:Class ;
                rdfs:subClassOf :IaaS ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasStorage ;
                                  owl:someValuesFrom <https://schema.org/TypeAndQuantityNode>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasMemory ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <https://schema.org/TypeAndQuantityNode>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://purl.org/goodrelations/v1#hasPriceSpecification> ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :CloudStorageTransactionsPriceSpecification
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasCPUcapacity ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <https://schema.org/TypeAndQuantityNode>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasMaxNumberOfDisks ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <https://schema.org/TypeAndQuantityNode>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasMaxStorageSize ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <https://schema.org/TypeAndQuantityNode>
                                ] ,
