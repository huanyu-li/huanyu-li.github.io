    <!-- http://purl.obolibrary.org/obo/CHEBI_33250 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CHEBI_33250">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/chebi/formula"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Atom</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/mdo/core/Structure -->

    <owl:Class rdf:about="https://w3id.org/mdo/core/Structure">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/mdo/structure/hasBasis"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/mdo/structure/Basis"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/mdo/structure/hasBasis"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/mdo/structure/Basis"/>
                    </owl:Restriction>
