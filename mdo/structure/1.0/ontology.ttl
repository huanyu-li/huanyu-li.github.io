###  https://w3id.org/mdo/structure/Basis
:Basis rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Class ;
                         owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasAngleVector ;
                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass :AngleTriple
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasLengthVector ;
                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass :LengthTriple
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ]
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasAxisVectors ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :AxisVectors
                                       ]
                                     )
                       ] ;
       rdfs:comment "A basis can be described by basis's length vector a, b, c and the intervector angles alpha, beta and gamma."@en .


###  https://w3id.org/mdo/structure/Composition
:Composition rdf:type owl:Class ;
             rdfs:comment "Composition is used to represent the composition of chemical elements of structures."@en .


###  https://w3id.org/mdo/structure/Lattice
:Lattice rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Class ;
                           owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasAngleVector ;
                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onClass :AngleTriple
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasLengthVector ;
                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onClass :LengthTriple
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ]
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasAxisVectors ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :AxisVectors
                                         ]
                                       )
                         ] ;
         rdfs:comment "Lattice is a set of points abstracted from basis. A three-dimensional lattice can ve visualized best as an infinite periodic array of points."@en .


###  https://w3id.org/mdo/structure/LengthTriple
:LengthTriple rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :hasLength_a ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:double
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasLength_b ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:double
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasLength_c ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:double
                              ] ;
              rdfs:comment "LengthTriple consists of three bases of a unit cell."@en .


###  https://w3id.org/mdo/structure/PointGroup
:PointGroup rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasPointGroupHMName ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ;
            rdfs:comment "A group of linear mappings of vector space, which is corresonpond to the group of motions in point space determining the symmetry of the macroscopic crystal, is called the point group of the crystal, furthermore of the crystal structure, and is also called the point group of its space group."@en ;
            rdfs:seeAlso "International Tables for Crystallography, Volume A, Fourth, revised edition, Section 8.1.5."@en .


###  https://w3id.org/mdo/structure/Site
:Site rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :hasSpecies ;
                        owl:someValuesFrom :Species
                      ] ;
      rdfs:comment "Each point in a lattice is a site."@en .


###  https://w3id.org/mdo/structure/SpaceGroup
:SpaceGroup rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasPointGroup ;
                              owl:someValuesFrom :PointGroup
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasSpaceGroupSymbol ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ;
            rdfs:comment "The symmetry group of a three-dimensional crystal pattern is called its space group."@en ;
            rdfs:seeAlso "International Tables for Crystallography, Volume !, Fourth, revised edition, Section 8.1.5."@en .


###  https://w3id.org/mdo/structure/Species
:Species rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasOxidationState ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:float
                         ] ;
         rdfs:comment "Species is the combination of atoms on each site."@en .


