#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/mdo/calculation/hasComputationalMethod
:hasComputationalMethod rdf:type owl:ObjectProperty ;
                        rdfs:domain <https://w3id.org/mdo/core/Calculation> ;
                        rdfs:range :ComputationalMethod ;
                        rdfs:comment "A calculation belongs to a specific Computational Chemical Method." .


###  https://w3id.org/mdo/calculation/hasXCFunctional
:hasXCFunctional rdf:type owl:ObjectProperty ;
                 rdfs:domain :DensityFunctionalTheoryMethod ;
                 rdfs:range :ExchangeCorrelationEnergyFunctional .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/mdo/calculation/hasPotentialFunction
:hasPotentialFunction rdf:type owl:DatatypeProperty ;
                      rdfs:domain :DensityFunctionalTheoryMethod .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/mdo/calculation/ComputationalMethod
:ComputationalMethod rdf:type owl:Class .


###  https://w3id.org/mdo/calculation/DensityFunctionalTheoryMethod
:DensityFunctionalTheoryMethod rdf:type owl:Class ;
                               rdfs:subClassOf :ComputationalMethod ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :hasXCFunctional ;
                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                 owl:onClass :ExchangeCorrelationEnergyFunctional
                                               ] ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :hasPotentialFunction ;
                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                 owl:onDataRange xsd:string
                                               ] ;
                               owl:disjointWith :Hartree-FockMethod .


###  https://w3id.org/mdo/calculation/ExchangeCorrelationEnergyFunctional
:ExchangeCorrelationEnergyFunctional rdf:type owl:Class .


###  https://w3id.org/mdo/calculation/GeneralizedGradientApproximation
:GeneralizedGradientApproximation rdf:type owl:Class ;
                                  rdfs:subClassOf :ExchangeCorrelationEnergyFunctional ;
                                  owl:disjointWith :HybridFunctionals ,
                                                   :LocalDensityApproximation ,
                                                   :metaGeneralizedGradientApproximation .


###  https://w3id.org/mdo/calculation/Hartree-FockMethod
:Hartree-FockMethod rdf:type owl:Class ;
                    rdfs:subClassOf :ComputationalMethod .


###  https://w3id.org/mdo/calculation/HybridFunctionals
:HybridFunctionals rdf:type owl:Class ;
                   rdfs:subClassOf :ExchangeCorrelationEnergyFunctional ;
                   owl:disjointWith :LocalDensityApproximation ,
                                    :metaGeneralizedGradientApproximation .


###  https://w3id.org/mdo/calculation/HybridGGA
:HybridGGA rdf:type owl:Class ;
           rdfs:subClassOf :HybridFunctionals ;
           owl:disjointWith :HybridmGGA .


###  https://w3id.org/mdo/calculation/HybridmGGA
:HybridmGGA rdf:type owl:Class ;
            rdfs:subClassOf :HybridFunctionals .


###  https://w3id.org/mdo/calculation/LocalDensityApproximation
:LocalDensityApproximation rdf:type owl:Class ;
                           rdfs:subClassOf :ExchangeCorrelationEnergyFunctional ;
                           owl:disjointWith :metaGeneralizedGradientApproximation .


###  https://w3id.org/mdo/calculation/metaGeneralizedGradientApproximation
:metaGeneralizedGradientApproximation rdf:type owl:Class ;
                                      rdfs:subClassOf :ExchangeCorrelationEnergyFunctional .


###  https://w3id.org/mdo/core/Calculation
<https://w3id.org/mdo/core/Calculation> rdf:type owl:Class ;
                                        rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasComputationalMethod ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :ComputationalMethod
                                                        ] .


