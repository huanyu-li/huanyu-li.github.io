                         rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasCartesianCoordinateY
:hasCartesianCoordinateY rdf:type owl:DatatypeProperty ;
                         rdfs:domain :CrystalLattice ;
                         rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasCartesianCoordinateZ
:hasCartesianCoordinateZ rdf:type owl:DatatypeProperty ;
                         rdfs:domain :CrystalLattice ;
                         rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasDSubShellElectronNumber
:hasDSubShellElectronNumber rdf:type owl:DatatypeProperty ;
                            rdfs:range [ rdf:type rdfs:Datatype ;
                                         owl:onDatatype xsd:integer ;
                                         owl:withRestrictions ( [ xsd:minInclusive 0
                                                                ]
                                                                [ xsd:maxInclusive 10
                                                                ]
                                                              )
                                       ] ;
                            rdfs:comment "The number of electrons at d subshell for a specific shell (M or N or O or P or Q)." .


###  https://w3id.org/mdo/structure/hasDensity
:hasDensity rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasElasticAnisotropy
:hasElasticAnisotropy rdf:type owl:DatatypeProperty ;
                      rdfs:domain :CrystalLattice ;
                      rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasEnergygap
:hasEnergygap rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:float .


