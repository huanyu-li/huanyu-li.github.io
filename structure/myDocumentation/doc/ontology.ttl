#################################################################

###  http://emmo.info/domains/emmo-material.owl#EMMO_4207e895_8b83_4318_996a_72cfb32acd94
<http://emmo.info/domains/emmo-material.owl#EMMO_4207e895_8b83_4318_996a_72cfb32acd94> rdf:type owl:Class .


###  http://purl.obolibrary.org/obo/CHEBI_33250
<http://purl.obolibrary.org/obo/CHEBI_33250> rdf:type owl:Class ;
                                             rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasAtomicMass ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:float
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasAtomicNumber ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:integer
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasAtomicRadius ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:float
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasAtomicSymbol ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:string
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasBlock ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:string
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasBoilingPointCelsius ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:float
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasBoilingPointKelvins ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:float
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasFirstIonizationEnergy ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:float
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasFullAtomicName ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:string
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasMeltingPointCelsius ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:float
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasMeltingPointKelvins ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:float
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasMendeleevNumber ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:integer
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasMolarVolume ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onDataRange xsd:float
                                                             ] ,
                                                             [ rdf:type owl:Restriction ;
