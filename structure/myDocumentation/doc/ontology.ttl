#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/mdo/structure/hasAtom
:hasAtom rdf:type owl:ObjectProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Composition
                                     :Species
                                   )
                     ] ;
         rdfs:range <http://purl.obolibrary.org/obo/CHEBI_33250> .


###  https://w3id.org/mdo/structure/hasBravaisLattice
:hasBravaisLattice rdf:type owl:ObjectProperty ;
                   rdfs:domain :CrystalLattice ;
                   rdfs:range :BravaisLattice .


###  https://w3id.org/mdo/structure/hasComposition
:hasComposition rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( <http://emmo.info/domains/emmo-material.owl#EMMO_4207e895_8b83_4318_996a_72cfb32acd94>
                                            <https://w3id.org/mdo/core/Structure>
                                          )
                            ] ;
                rdfs:range :Composition .


###  https://w3id.org/mdo/structure/hasCrystalBonding
:hasCrystalBonding rdf:type owl:ObjectProperty ;
                   rdfs:domain :Species ;
                   rdfs:range :CrystalBonding .


###  https://w3id.org/mdo/structure/hasElement
:hasElement rdf:type owl:ObjectProperty ;
            rdfs:domain :Species ;
            rdfs:range <http://purl.obolibrary.org/obo/CHEBI_33250> .


###  https://w3id.org/mdo/structure/hasLattice
:hasLattice rdf:type owl:ObjectProperty ;
            rdfs:domain <https://w3id.org/mdo/core/Structure> ;
            rdfs:range :CrystalLattice .


###  https://w3id.org/mdo/structure/hasLatticeType
:hasLatticeType rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :BravaisLattice
                                            :CrystalLattice
                                          )
                            ] ;
                rdfs:range :LatticeType .


###  https://w3id.org/mdo/structure/hasPGType
:hasPGType rdf:type owl:ObjectProperty ;
           rdfs:domain :PointGroup ;
           rdfs:range :PointGroupType .


###  https://w3id.org/mdo/structure/hasPointGroup
:hasPointGroup rdf:type owl:ObjectProperty ;
               rdfs:domain :CrystalLattice ,
                           :SpaceGroup ;
               rdfs:range :PointGroup .


###  https://w3id.org/mdo/structure/hasSPType
:hasSPType rdf:type owl:ObjectProperty ;
           rdfs:domain :SpaceGroup ;
           rdfs:range :SpaceGroupType .


###  https://w3id.org/mdo/structure/hasSite
:hasSite rdf:type owl:ObjectProperty ;
         rdfs:domain <https://w3id.org/mdo/core/Structure> ;
         rdfs:range :Site .


###  https://w3id.org/mdo/structure/hasSpaceGroup
:hasSpaceGroup rdf:type owl:ObjectProperty ;
               rdfs:domain <https://w3id.org/mdo/core/Structure> ;
               rdfs:range :SpaceGroup .


###  https://w3id.org/mdo/structure/hasSpecies
:hasSpecies rdf:type owl:ObjectProperty ;
            rdfs:domain :Site ;
            rdfs:range :Species .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/mdo/structure/hasAngle_alpha
:hasAngle_alpha rdf:type owl:DatatypeProperty ;
                rdfs:domain :BravaisLattice ,
                            :CrystalLattice ,
                            :LatticeType ;
                rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasAngle_beta
:hasAngle_beta rdf:type owl:DatatypeProperty ;
               rdfs:domain :BravaisLattice ,
                           :CrystalLattice ,
                           :LatticeType ;
               rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasAngle_gamma
:hasAngle_gamma rdf:type owl:DatatypeProperty ;
                rdfs:domain :BravaisLattice ,
                            :CrystalLattice ,
                            :LatticeType ;
                rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasAtomicMass
:hasAtomicMass rdf:type owl:DatatypeProperty ;
               rdfs:domain <http://purl.obolibrary.org/obo/CHEBI_33250> ;
               rdfs:range xsd:float ;
               rdfs:comment "The atomic mass of an element." .


###  https://w3id.org/mdo/structure/hasAtomicNumber
:hasAtomicNumber rdf:type owl:DatatypeProperty ;
                 rdfs:domain <http://purl.obolibrary.org/obo/CHEBI_33250> ;
                 rdfs:range [ rdf:type rdfs:Datatype ;
                              owl:onDatatype xsd:integer ;
                              owl:withRestrictions ( [ xsd:minInclusive 1
                                                     ]
                                                     [ xsd:maxInclusive 118
                                                     ]
                                                   )
                            ] ;
                 rdfs:comment "The atomic number of an element." .


###  https://w3id.org/mdo/structure/hasAtomicRadius
:hasAtomicRadius rdf:type owl:DatatypeProperty ;
                 rdfs:domain <http://purl.obolibrary.org/obo/CHEBI_33250> ;
                 rdfs:range xsd:float ;
                 rdfs:comment "The atomic radius of an element" .


###  https://w3id.org/mdo/structure/hasAtomicSymbol
:hasAtomicSymbol rdf:type owl:DatatypeProperty ;
                 rdfs:domain <http://purl.obolibrary.org/obo/CHEBI_33250> ;
                 rdfs:range xsd:string ;
                 rdfs:comment "The atomic symbol of an element." .


###  https://w3id.org/mdo/structure/hasBandgap
:hasBandgap rdf:type owl:DatatypeProperty ;
            owl:equivalentProperty :hasEnergygap ;
            rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasBasis_a
:hasBasis_a rdf:type owl:DatatypeProperty ;
            rdfs:domain :BravaisLattice ,
                        :CrystalLattice ,
                        :LatticeType ;
            rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasBasis_b
:hasBasis_b rdf:type owl:DatatypeProperty ;
            rdfs:domain :BravaisLattice ,
                        :CrystalLattice ,
                        :LatticeType ;
            rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasBasis_c
:hasBasis_c rdf:type owl:DatatypeProperty ;
            rdfs:domain :BravaisLattice ,
                        :CrystalLattice ,
                        :LatticeType ;
            rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasBlock
:hasBlock rdf:type owl:DatatypeProperty ;
          rdfs:domain <http://purl.obolibrary.org/obo/CHEBI_33250> ;
          rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:oneOf [ rdf:type rdf:List ;
                                   rdf:first "d" ;
                                   rdf:rest [ rdf:type rdf:List ;
                                              rdf:first "f" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "p" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "s" ;
                                                                    rdf:rest rdf:nil
                                                                  ]
                                                       ]
                                            ]
                                 ]
                     ] .


###  https://w3id.org/mdo/structure/hasBoilingPointCelsius
:hasBoilingPointCelsius rdf:type owl:DatatypeProperty ;
                        rdfs:domain <http://purl.obolibrary.org/obo/CHEBI_33250> ;
                        rdfs:range xsd:float ;
                        rdfs:comment "The boiling point of an element at Celsius temperature." .


###  https://w3id.org/mdo/structure/hasBoilingPointKelvins
:hasBoilingPointKelvins rdf:type owl:DatatypeProperty ;
                        rdfs:domain <http://purl.obolibrary.org/obo/CHEBI_33250> ;
                        rdfs:range xsd:float ;
                        rdfs:comment "The boiling point of an element at Kelvins temperature." .


###  https://w3id.org/mdo/structure/hasBulkModulus
:hasBulkModulus rdf:type owl:DatatypeProperty ;
                rdfs:domain :CrystalLattice ;
                rdfs:range xsd:float .


###  https://w3id.org/mdo/structure/hasCartesianCoordinateX
:hasCartesianCoordinateX rdf:type owl:DatatypeProperty ;
