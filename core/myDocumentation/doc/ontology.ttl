<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/mdo/core/PartOf
:PartOf rdf:type owl:ObjectProperty ;
        rdfs:domain :Structure ;
        rdfs:range <http://emmo.info/domains/emmo-material.owl#EMMO_4207e895_8b83_4318_996a_72cfb32acd94> .


###  https://w3id.org/mdo/core/approximateBy
:approximateBy rdf:type owl:ObjectProperty ;
               rdfs:domain :MaterialsData ;
               rdfs:range :Structure .


###  https://w3id.org/mdo/core/decomposeTo
:decomposeTo rdf:type owl:ObjectProperty ;
             rdfs:domain :MaterialsData ;
             rdfs:range :MaterialsData .


###  https://w3id.org/mdo/core/describedBy
:describedBy rdf:type owl:ObjectProperty ;
             rdfs:domain <http://emmo.info/domains/emmo-material.owl#EMMO_4207e895_8b83_4318_996a_72cfb32acd94> ;
             rdfs:range :MaterialsData .


###  https://w3id.org/mdo/core/hasCalculatedProperty
:hasCalculatedProperty rdf:type owl:ObjectProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :Calculation
                                                   :Structure
                                                 )
                                   ] ;
                       rdfs:range :CalculatedProperty .


###  https://w3id.org/mdo/core/hasInputStructure
:hasInputStructure rdf:type owl:ObjectProperty ;
                   rdfs:domain :Calculation ;
                   rdfs:range :Structure .


###  https://w3id.org/mdo/core/hasOutputStructure
:hasOutputStructure rdf:type owl:ObjectProperty ;
                    rdfs:domain :Calculation ;
                    rdfs:range :Structure .


###  https://w3id.org/mdo/core/hasPhysicalProperty
:hasPhysicalProperty rdf:type owl:ObjectProperty ;
                     rdfs:domain :Structure ;
                     rdfs:range :PhysicalProperty .


###  https://w3id.org/mdo/core/hasUnit
:hasUnit rdf:type owl:ObjectProperty ;
         rdfs:domain :Property ;
         rdfs:range <http://purl.obolibrary.org/obo/Unit> .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/mdo/core/hasID
:hasID rdf:type owl:DatatypeProperty ;
       rdfs:domain :MaterialsData .


###  https://w3id.org/mdo/core/hasPropertyValue
:hasPropertyValue rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Property ;
                  rdfs:range xsd:double .


#################################################################
#    Classes
#################################################################

###  http://emmo.info/domains/emmo-material.owl#EMMO_4207e895_8b83_4318_996a_72cfb32acd94
<http://emmo.info/domains/emmo-material.owl#EMMO_4207e895_8b83_4318_996a_72cfb32acd94> rdf:type owl:Class ;
                                                                                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty :describedBy ;
                                                                                                         owl:someValuesFrom :MaterialsData
                                                                                                       ] ;
                                                                                       rdfs:label "material" .


###  http://purl.obolibrary.org/obo/Unit
<http://purl.obolibrary.org/obo/Unit> rdf:type owl:Class .


###  https://w3id.org/mdo/core/CalculatedProperty
:CalculatedProperty rdf:type owl:Class ;
                    rdfs:subClassOf :Property .


###  https://w3id.org/mdo/core/Calculation
:Calculation rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasCalculatedProperty ;
                               owl:someValuesFrom :CalculatedProperty
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasInputStructure ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Structure
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasOutputStructure ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Structure
                             ] .


###  https://w3id.org/mdo/core/MaterialsData
:MaterialsData rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty :decomposeTo ;
                                 owl:someValuesFrom :MaterialsData
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :approximateBy ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :Calculation
                               ] .


###  https://w3id.org/mdo/core/PhysicalProperty
:PhysicalProperty rdf:type owl:Class ;
                  rdfs:subClassOf :Property .


###  https://w3id.org/mdo/core/Property
:Property rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasUnit ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass <http://purl.obolibrary.org/obo/Unit>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasPropertyValue ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:double
                          ] .


###  https://w3id.org/mdo/core/Structure
:Structure rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :PartOf ;
                             owl:someValuesFrom <http://emmo.info/domains/emmo-material.owl#EMMO_4207e895_8b83_4318_996a_72cfb32acd94>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasCalculatedProperty ;
                             owl:someValuesFrom :CalculatedProperty
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasPhysicalProperty ;
                             owl:someValuesFrom :PhysicalProperty
                           ] .


